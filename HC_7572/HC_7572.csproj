<Project Sdk="Microsoft.NET.Sdk.Web">

	<PropertyGroup>
		<TargetFramework>net8.0</TargetFramework>
		<Nullable>enable</Nullable>
		<ImplicitUsings>enable</ImplicitUsings>
		<UserSecretsId>ee9efdb6-c99e-429e-a8b4-40ba966078e2</UserSecretsId>
		<DockerDefaultTargetOS>Linux</DockerDefaultTargetOS>
		<NoWarn>$(NoWarn);GD0001</NoWarn>
	</PropertyGroup>

	<ItemGroup>
		<PackageReference Include="HotChocolate.AspNetCore" Version="14.0.0-p.188" />
		<PackageReference Include="HotChocolate.Data" Version="14.0.0-p.188" />
		<PackageReference Include="HotChocolate.Types.Analyzers" Version="14.0.0-p.188" />
	</ItemGroup>

</Project>
